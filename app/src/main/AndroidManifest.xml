<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.storage">

    <application android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name=".WorkWithFileActivity" />
        <activity android:name=".ExternalStorageActivity" />
        <activity android:name=".ScopedStorageActivity" />
        <activity android:name=".FileWithoutPermissionActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>